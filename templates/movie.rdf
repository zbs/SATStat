<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
   xmlns:dbpedia-prop="http://dbpedia.org/property/"
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:foaf="http://xmlns.com/foaf/0.1/"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
>
    <rdf:Description rdf:about="{{ movie['uri'] }}">
        <rdf:type rdf:resource="http://dbpedia.org/ontology/Film"/>
        {% if 'title' in movie %}
          <dcterms:title>{{ movie['title'] }}</dcterms:title>
        {% end %}
        {% if 'synopsis' in movie %}
          <dcterms:description>{{ movie['synopsis'] }}</dcterms:description>
        {% end %}
        {% if 'actors' in movie %}
            {% for actor_uri in movie['actors'] %}
           <dbpedia-prop:starring_of rdf:resource="{{ actor_uri }}"/>
            {% end %}
        {% end %}
    </rdf:Description>

</rdf:RDF>